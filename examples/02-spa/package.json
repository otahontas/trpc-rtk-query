{
  "name": "trpc-rtk-query-spa-example",
  "version": "1.0.0",
  "description": "Single Page Application example for trpc-rtk-query",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "tsx watch src/server/index.ts",
    "dev:client": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "start": "NODE_ENV=production node dist/server/index.js"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.5",
    "@trpc/client": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.22.1",
    "trpc-rtk-query": "^0.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.19",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.4"
  }
}
